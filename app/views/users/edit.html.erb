<h1>Update User</h1>

<%= form_for @user do |f| %>
	<%= f.label "First Name" %>
	<%= f.text_field :fname %><br><br>
	<%= f.label "Last Name" %>
	<%= f.text_field :lname %><br><br>
	<%= f.label "Username" %>
	<%= f.text_field :username %><br><br>
	<%= f.label "Email" %>
	<%= f.text_field :email %><br><br>
	<%= f.label "Password" %>
	<%= f.password_field :password %><br><br>
	<br>
	<%= f.submit :update %>
	<br><br>
	<%= link_to "Go Back", users_path %>
<% end %>
